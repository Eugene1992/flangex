<%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %>
<section class="success" id="about">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2><spring:message code="message.main.about.title"/></h2>
                <hr class="star-light">
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-lg-offset-2">
                <p><spring:message code="message.main.about.left"/></p>
            </div>
            <div class="col-lg-4">
                <p><spring:message code="message.main.about.right"/></p>
            </div>
        </div>
    </div>
</section>